{
	"name": "doctor-large",
	"version": "1.0.0",
	"description": "He's here.",
	"main": "dist/index.js",
	"type": "module",
	"scripts": {
		"prebuild": "npx rimraf dist",
		"build": "tsc",
		"precommands:clear": "npm run build",
		"commands:clear": "node -r dotenv/config scripts/clearCommands.js",
		"precommands:put": "npm run build",
		"commands:put": "node -r dotenv/config scripts/putCommands.js",
		"format": "prettier --write \"**/*.+(js|ts|json)\"",
		"lint:fix": "eslint src/** --ext .ts --fix",
		"lint": "eslint src/** --ext .ts",
		"prestart": "npm run build",
		"start": "node -r dotenv/config ."
	},
	"author": "Drango",
	"license": "MIT",
	"dependencies": {
		"@prisma/client": "^5.1.0",
		"common-tags": "^2.0.0-alpha.1",
		"discord.js": "^14.12.1",
		"dotenv": "^16.3.1",
		"ms": "^2.1.3"
	},
	"devDependencies": {
		"@types/common-tags": "^1.8.1",
		"@types/eslint": "^8.44.1",
		"@types/ms": "^0.7.31",
		"@typescript-eslint/eslint-plugin": "^6.2.1",
		"@typescript-eslint/parser": "^6.2.1",
		"discord-api-types": "^0.37.51",
		"eslint": "^8.46.0",
		"eslint-config-prettier": "^8.9.0",
		"eslint-plugin-import": "^2.28.0",
		"eslint-plugin-prettier": "^5.0.0",
		"eslint-plugin-unicorn": "^48.0.1",
		"prettier": "^3.0.0",
		"prisma": "^5.1.0",
		"typescript": "^5.1.6"
	},
	"imports": {
		"#components": "./dist/components/index.js",
		"#constants": "./dist/constants.js",
		"#database/*": "./dist/database/*",
		"#helpers": "./dist/helpers/index.js",
		"#logger": "./dist/logger/logger.js",
		"#modules/*": "./dist/modules/*",
		"#scripts/*": "./dist/scripts/*",
		"#typings": "./dist/typings/index.d.js"
	}
}
